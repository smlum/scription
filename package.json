  },
  "homepage": "https://github.com/smlum/scription",
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "bulma": "^0.8.0",
    "bulma-switch": "^2.0.0",
    "connect-browser-sync": "^2.1.0",
    "copyfiles": "^2.3.0",
    "electron": "^8.5.2",
    "electron-packager": "^14.2.1",
    "http-server": "^0.12.3",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
